<!-- http://wiki.collectionspace.org/display/deploy/Cataloging+Schema+Extension+-+Variable+Media -->
<record id="collection-object">
    <services-record-path id="variablemedia">collectionobjects_variablemedia:http://collectionspace.org/services/collectionobject/domain/variablemedia,collectionobjects_variablemedia</services-record-path>

    <section id="filmAndVideoComponentsInformation">
        <repeat id="originalFormatGroupList/originalFormatGroup" section="variablemedia">
            <field id="originalFormat" autocomplete="vocab-originalformat" ui-type="enum" section="variablemedia">
                <selector>collection-object-originalFormat</selector>
            </field>
            <field id="originalAudioFormat" section="variablemedia">
                <!-- needs term list -->
                <selector>collection-object-originalAudioFormat</selector>
            </field>
        </repeat>
        <repeat id="copiesProvidedOnAcquisition" section="variablemedia">
            <field id="copyProvidedOnAcquisition" section="variablemedia">
                <!-- needs term list -->
                <selector>collection-object-copyProvidedOnAcquisition</selector>
            </field>
        </repeat>
    </section>

    <section id="digitalWorksInformation">
        <repeat id="digitalWorksGroupList/digitalWorksGroup" section="variablemedia">
            <repeat id="programmingLanguageSubGroupList/programmingLanguageSubGroup" section="variablemedia">
                <field id="programmingLanguage" autocomplete="vocab-programminglanguage" ui-type="enum" section="variablemedia">
                    <selector>collection-object-programmingLanguage</selector>
                </field>
                <field id="authoringEnvironment" autocomplete="vocab-authoringenvironment" ui-type="enum" section="variablemedia">
                    <selector>collection-object-authoringEnvironment</selector>
                </field>
            </repeat>
            <repeat id="compressionSystemsUsed" section="variablemedia">
                <field id="compressionSystemUsed" section="variablemedia">
                    <selector>collection-object-compressionSystemUsed</selector>
                </field>
            </repeat>
            <repeat id="ratioFormats" section="variablemedia">
                <field id="ratioFormat" autocomplete="vocab-ratioformat" ui-type="enum" section="variablemedia">
                    <selector>collection-object-ratioFormat</selector>
                </field>
            </repeat>
            <field id="nonStandardCode" section="variablemedia">
                <selector>collection-object-nonStandardCode</selector>
            </field>
            <field id="sourcesOfExternalData" section="variablemedia">
                <selector>collection-object-sourcesOfExternalData</selector>
            </field>
            <repeat id="preferredScreenResolutionSubGroupList/preferredScreenResolutionSubGroup" section="variablemedia">
                <field id="preferredScreenResolution" autocomplete="vocab-preferredscreenresolution" ui-type="enum" section="variablemedia">
                    <selector>collection-object-preferredScreenResolution</selector>
                </field>
                <field id="preferredColorPalette" autocomplete="vocab-preferredcolorpalette" ui-type="enum" section="variablemedia">
                    <selector>collection-object-preferredColorPalette</selector>
                </field>
            </repeat>
            <repeat id="creationHardwares" section="variablemedia">
                <field id="creationHardware" section="variablemedia">
                    <!-- needs term list -->
                    <selector>collection-object-creationHardware</selector>
                </field>
            </repeat>
            <repeat id="playbackHardwares" section="variablemedia">
                <field id="playbackHardware" autocomplete="vocab-playbackhardware" ui-type="enum" section="variablemedia">
                    <selector>collection-object-playbackHardware</selector>
                </field>
            </repeat>
        </repeat>
    </section>

</record>
